; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={
}

[application]

config/name="ProjectSkripsi"
run/main_scene="res://Scene/StartLoading.tscn"
config/icon="res://icon.png"
config/auto_accept_quit=false
config/quit_on_go_back=false

[autoload]

Global="*res://Script/Global.gd"
Database="*res://Script/Database.gd"

[display]

window/size/width=720
window/size/height=1280
window/handheld/orientation="portrait"
window/stretch/mode="2d"

[editor_plugins]

enabled=PoolStringArray( "res://addons/godot-sqlite/plugin.cfg" )

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

vram_compression/import_etc=true
environment/default_clear_color=Color( 0.376471, 0.356863, 0.368627, 1 )
environment/default_environment="res://default_env.tres"
quality/dynamic_fonts/use_oversampling=false
